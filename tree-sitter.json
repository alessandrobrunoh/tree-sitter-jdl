{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "jdl",
      "camelcase": "Jdl",
      "title": "Jdl",
      "scope": "source.jdl",
      "file-types": [
        "jdl"
      ],
      "injection-regex": "^jdl$",
      "class-name": "TreeSitterJdl"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "Jdl grammar for tree-sitter",
    "authors": [
      {
        "name": "Alessandro Bruno",
        "email": "alessandro.brunoh@gmail.com"
      }
    ],
    "links": {
      "repository": "https://github.com/tree-sitter/tree-sitter-jdl"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}